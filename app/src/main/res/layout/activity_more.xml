<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/style_linearlayout_pd_fillet_white"
        tools:ignore="MissingConstraints"
        android:id="@+id/linearLayout2">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/more_tv_common"
            android:padding="@dimen/dp_16"
            android:text="常用"
            android:textColor="@color/colorBlack"
            android:textSize="@dimen/sp_18"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/more_ll_common_content0"
            android:layout_marginStart="@dimen/dp_32"
            android:layout_gravity="left"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="@dimen/dp_75"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/dp_48"
                    android:layout_height="@dimen/dp_48"
                    android:id="@+id/more_iv_call"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    android:paddingTop="@dimen/dp_10"
                    android:onClick="onClick"
                    android:src="@drawable/calls209blue"
                    tools:ignore="OnClick"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/calls"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout3"
        android:orientation="vertical"
        android:background="@drawable/style_linearlayout_pd_fillet_white"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="@dimen/dp_8"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/more_tv_more"
            android:padding="@dimen/dp_16"
            android:text="更多"
            android:textColor="@color/colorBlack"
            android:textSize="@dimen/sp_18"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/more_ll_more_content0"
            android:layout_marginStart="@dimen/dp_32"
            android:layout_gravity="left"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="@dimen/dp_75"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center">
                <ImageView
                    android:layout_width="@dimen/dp_48"
                    android:layout_height="@dimen/dp_48"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    android:paddingTop="@dimen/dp_10"
                    android:onClick="onClick"
                    android:src="@drawable/calls209blue"
                    tools:ignore="OnClick"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/calls"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <view class="androidx.appcompat.app.AlertController$RecycleListView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/more_rl_more"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">
    </view>
</androidx.constraintlayout.widget.ConstraintLayout>